{
	"compilerOptions": {
		"lib": [
			"ES2024",
		],
		"target": "ES2024",
		"module": "ESNext",
		"moduleDetection": "force",
		"moduleResolution": "bundler",
		"verbatimModuleSyntax": true,
		"strict": true,
		"skipLibCheck": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noPropertyAccessFromIndexSignature": true
	}
}